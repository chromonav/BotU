<div class="container prodcontain">
  <div class="row">

    <div class="col-md-12">
       <table class="prodtable table table-list-search">
         <thead class="headrow">
           <tr>
             <div class="row">
             <th id="heading" colspan="2">Stores</th>
             <!-- <td class="col-md-2"></td><td class="col-md-5"></td> -->
             <td align="right" class="col-md-5">
           <form action="#" method="get" class="sear">
               <div class="input-group">
                     <input class="form-control" id="system-search" name="q" placeholder="Search for Store" required>
                   <!-- <span class="input-group-btn">
                       <button type="submit" class="btn btn-default"><span class="glyphicon glyphicon-search"></span></button>
                   </span> -->
               </div>
           </form>
         </td>
       </div>
         </tr>
         </thead>
              <thead>
                  <tr>
                      <th>Id</th>
                      <th>Store</th>
                      <th>Location</th>
                    </tr>
                </thead>
                    <tbody>

                        {{#each data}}
                            <tr>
                            <td>{{this.sid}}</td>
                            <td>{{this.sname}}</td>
                            <td>{{this.address}}</td>
                            </tr>
                        {{/each}}

                    </tbody>
            </table>
    </div>
  </div>
</div>
